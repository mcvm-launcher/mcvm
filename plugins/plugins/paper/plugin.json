{
	"name": "Paper",
	"description": "Install Paper and Folia instances",
	"protocol_version": 3,
	"hooks": {
		"on_instance_setup": {
			"executable": "${PLUGIN_DIR}/mcvm_plugin_paper"
		},
		"add_supported_game_modifications": {
			"constant": {
				"server_types": [
					"paper",
					"folia"
				]
			}
		}
	}
}